{
    "name": "unicodepalette",
    "displayName": "Unicode Palette",
    "description": "Command palette for entering unicode characters. Useful for symbols, emoji and other characters that you might not have on your keyboard",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:unicodepalette.insertCharacter"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "unicodepalette.insertCharacter",
                "category": "Character Palette",
                "title": "Insert Character"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/papaparse": "^5.2.5",
        "@types/vscode": "^1.53.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "papaparse": "^5.3.0",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "avsc": "^5.5.3"
    }
}
